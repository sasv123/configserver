
# This section has properties related to gemfire connectivity for PSG
#
ccp.gemfire.session.locator=localhost[10334]
ccp.gemfire.session.listeners=com.citigroup.ccp.session.listener.SessionListener
ccp.gemfire.session.strategy=header

# This section has properties related to supporting Country Code, Channel ID, Business Code & Accept-Langauge Configuration
#
#ccp.security.validation.supportingCountryCodes=US,AU
ccp.security.validation.countryCodes.supported=US,AU,GB


#ccp.security.validation.supportingBusinessCodes.AU.default=GCB
ccp.security.validation.businessCodes.AU.supported=GCB,XYZ,GCBA
ccp.security.validation.businessCodes.AU.default=GCB

ccp.security.validation.businessCodes.GB.supported=GCB,XYZ
ccp.security.validation.businessCodes.GB.default=GCB


#ccp.security.validation.supportingChannelIDs.AU.GCB=CML,HBK,MBK,ECL
ccp.security.validation.channelIDs.AU.GCB.supported=CML,HBK,MBK,ECL
ccp.security.validation.channelIDs.AU.GCBA.supported=CML,HBK,MBK,ECL
ccp.security.validation.channelIDs.GB.GCB.supported=CML,HBK,MBK,ECL,b11111

ccp.security.validation.acceptLanguage.AU.GCB.supported=da,us-gb,us
ccp.security.validation.acceptLanguage.AU.GCB.default=us

#ccp.security.validation.supportingBusinessCodes.US.default=GCB
ccp.security.validation.businessCodes.US.supported=GCB
ccp.security.validation.businessCodes.US.default=GCB

#ccp.security.validation.supportingChannelIDs.US.GCB=HBK,MBK
ccp.security.validation.channelIDs.US.GCB.supported=CML,HBK,MBK,ECL

ccp.security.validation.acceptLanguage.US.GCB.supported=us-en, en-IN, us     
ccp.security.validation.acceptLanguage.US.GCB.default=us


ccp.security.auth.policy_file_target_path=
ccp.security.auth.config_file_target_path=
ccp.profile.policyfile.directory=C:/db/json

#ccp.security.api.loginAPIs.locallogin.url=/app/login
#ccp.security.api.loginAPIs.locallogin.defaultRole=CUSTOMER
#ccp.security.api.loginAPIs.locallogin.httpMethods=POST,GET

ccp.security.api.logoutAPIs.locallogin.url=/app/logout
ccp.security.api.logoutAPIs.locallogin.httpMethods=DELETE

ccp.security.api.sessionExclusionAPIs.demo.url=/app/contentService
ccp.security.api.sessionExclusionAPIs.demo.httpMethods=GET


ccp.security.api.sessionExclusionAPIs.productcodes.url=/app/login
ccp.security.api.sessionExclusionAPIs.productcodes.httpMethods=POST